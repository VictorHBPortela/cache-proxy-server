{
  "name": "cache-proxy-server",
  "version": "1.0.0",
  "description": "A caching proxy server built with Node.js, Express, and Redis",
  "main": "index.js",
  "type": "module",
  "bin": {
    "cache-proxy-server": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx watch index.ts",
    "build": "npm run lint && tsc",
    "start": "node dist/index.js",
    "link": "npm run build && npm link",
    "unlink": "npm unlink -g cache-proxy-server",
    "serve": "npm run build && node dist/index.js",
    "proxy": "npm run build && node dist/index.js",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix"
  },
  "keywords": [
    "proxy",
    "cache",
    "redis",
    "express",
    "cli",
    "http"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.0",
    "express": "^5.1.0",
    "redis": "^5.8.2",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
